{
  "name": "typescript-starter",
  "version": "0.1.0",
  "description": "A TypeScript starter project with many common NPM dev packages, but no runtime dependencies.",
  "scripts": {
    "test": "jest --watchAll --colors",
    "test:coverage": "jest --watchAll --colors --coverage",
    "build": "tsc",
    "build:watch": "tsc -w",
    "build:docs": "tsc && typedoc src --out build/docs",
    "monitor": "nodemon build/main.js"
  },
  "keywords": [
    "typescript",
    "starter"
  ],
  "author": "Kerim GÃ¼ney <kerim@gueney.io> (https://gueney.io)",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.14",
    "@types/node": "^12.0.8",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.2"
  }
}
